{
  "name": "edhtop16",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "generate:db": "rm -f edhtop16.db && node -r ts-node/register scripts/generate-database.ts",
    "generate:dbtypes": "DATABASE_URL=edhtop16.db kysely-codegen --out-file=./__generated__/db/types.d.ts",
    "generate:schema": "node -r ts-node/register scripts/print-schema.ts",
    "generate:relay": "relay-compiler",
    "generate:river": "node --experimental-strip-types scripts/rivergen.mts",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.tsx",
    "build": "npm run build:client && npm run build:server",
    "dev": "node --experimental-strip-types devserver.mts",
    "start": "NODE_ENV=production node --experimental-strip-types server.mts"
  },
  "imports": {
    "#src/*": "./src/*",
    "#genfiles/*": "./__generated__/*"
  },
  "dependencies": {
    "@graphql-yoga/plugin-persisted-operations": "^3.14.0",
    "@heroicons/react": "^2.1.5",
    "@pothos/core": "^4.3.0",
    "@pothos/plugin-dataloader": "^4.3.0",
    "@pothos/plugin-relay": "^4.3.0",
    "@types/express-session": "^1.18.2",
    "@unhead/react": "^2.0.11",
    "better-sqlite3": "^11.7.2",
    "classnames": "^2.5.1",
    "dataloader": "^2.2.2",
    "date-fns": "^3.6.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "fuse.js": "^7.0.0",
    "graphql": "^16.8.1",
    "graphql-yoga": "^5.13.4",
    "kysely": "^0.27.5",
    "mongodb": "^6.3.0",
    "picocolors": "^1.1.1",
    "radix3": "^1.1.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-relay": "^19.0.0",
    "relay-runtime": "^19.0.0",
    "serialize-javascript": "^6.0.2",
    "ts-node": "^10.9.2",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@faker-js/faker": "^9.0.3",
    "@reverecre/promise": "^0.1.0",
    "@tailwindcss/vite": "^4.1.11",
    "@types/better-sqlite3": "^7.6.12",
    "@types/express": "^5.0.3",
    "@types/node": "20.3.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@types/react-relay": "^18.2.1",
    "@types/relay-runtime": "^19.0.2",
    "@types/serialize-javascript": "^5.0.4",
    "@unhead/addons": "^2.0.11",
    "@vitejs/plugin-react": "^4.6.0",
    "babel-plugin-relay": "^16.2.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "kysely-codegen": "^0.17.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "relay-compiler": "^20.0.0",
    "relay-config": "^12.0.1",
    "tailwindcss": "^4.1.10",
    "ts-morph": "^26.0.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.35.1",
    "undici": "^7.0.0",
    "vite": "^6.3.5",
    "vite-plugin-cjs-interop": "^2.2.0"
  }
}
